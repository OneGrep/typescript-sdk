{
  "name": "@onegrep/langchain",
  "version": "0.0.1",
  "private": false,
  "type": "module",
  "license": "MIT",
  "author": "OneGrep, Inc.",
  "homepage": "https://onegrep.dev",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/onegrep/typescript-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/onegrep/typescript-sdk/issues"
  },
  "types": "dist/index.d.ts",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "exports": {
    "require": "./dist/index.cjs",
    "import": "./dist/index.js"
  },
  "files": [
    "dist/**/*"
  ],
  "outputs": [
    "dist/**/*"
  ],
  "scripts": {
    "check-types": "tsc -p tsconfig.check.json --noEmit",
    "build": "rollup -c",
    "licenses": "pnpm licenses list --prod --long > .licenses.txt",
    "package": "mkdir -p pack && pnpm pack --pack-destination pack",
    "package:dryrun": "pnpm pack --pack-destination pack --dry-run",
    "test": "LOG_MODE=console LOG_LEVEL=debug vitest --config vitest.config.ts --disable-console-intercept ",
    "test:watch": "LOG_MODE=console LOG_LEVEL=debug vitest --config vitest.config.ts --disable-console-intercept --watch",
    "clean:dist": "rimraf ./dist",
    "clean:modules": "rimraf ./node_modules",
    "clean:pack": "rimraf ./pack"
  },
  "devDependencies": {
    "@repo/typescript-config": "workspace:*",
    "@repo/onegrep-api-client": "workspace:*",
    "@repo/utils": "workspace:*",
    "@rollup/plugin-commonjs": "^28.0.2",
    "@rollup/plugin-json": "^6.0.1",
    "@rollup/plugin-node-resolve": "^16.0.0",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.1.2",
    "@types/eslint": "^8.56.0",
    "@types/node": "^22.13.4",
    "dotenv": "^16.4.7",
    "tsup": "^8.3.6",
    "rollup": "^4.34.9",
    "rollup-plugin-dts": "^6.1.1",
    "typescript": "latest",
    "vitest": "^3.0.5",
    "pino": "^9.6.0",
    "pino-pretty": "^13.0.0",
    "thread-stream": "^1.0.0"
  },
  "dependencies": {
    "@onegrep/sdk": "workspace:*",
    "@langchain/core": "^0.3.40",
    "@modelcontextprotocol/sdk": "^1.5.0",
    "eventsource": "^3.0.5",
    "zod": "^3.24.2"
  },
  "peerDependencies": {
    "dotenv": "^16.4.7",
    "chalk": "^5.3.0"
  }
}
